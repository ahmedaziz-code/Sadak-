{% block body %}
    <div>
        <img style="vertical-align:middle" src="data:image/png;base64," alt="S">
        <span style="font:bold;font-size: 35px;padding-left: 18%">
            Facture</span></div>

    <br><br><br>
    <h5 style="font-size: 16px">Achat effectué le : {{ date }}</h5>
    <p style="color: red">Veuillez conserver ce document jusqu'à ce que votre achat soit terminé.</p>
    <br><br>
    <h2>Destinataire </h2>
    <p style="font-size: 18px"> Commande Num : {{ commande }} <br>
        Client : {{ usernom }} {{ userprenom }}<br>
        Adresse : {{ adresse }} </p>
    <br>
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }
        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
    <br>
    <table class="table" border ="1">
        <thead>
        <tr>
            <th>Produit</th>
            <th>Prix unitaire</th>
            <th>Quantite</th>
            <th>Prix</th>
        </tr>
        </thead>
        <tbody>
        {% for panier in panier %}
            <tr>
                <td>{{ panier.refProduit.nomProduit }}</td>
                <td>{{ panier.refProduit.prixProduit }} DT </td>
                <td>{{ panier.quantiteProduit }}</td>
                <td>{{ panier.quantiteProduit * panier.refProduit.prixProduit }} DT</td>
            </tr>
        {% endfor %}
        <tr>
            <td></td>
            <td></td>
            <td>Frais de Livraison</td>
            <td>{{ frais }} DT </td>
        </tr>
        <tr>
            <td></td>
            <td></td>
            <td>Prix Total</td>
            <td>{{ prix }} DT </td>
        </tr>

        </tbody>
    </table>
{% endblock %}
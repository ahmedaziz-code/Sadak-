{% extends "base-front.html.twig" %}

{% block famille %}

    <div>
        <table class="styled-table" style="margin: auto">
            <thead>
            <tr>
                <td>Nom Famille</td>
                <td>Etat Famille</td>
                <td>Nombre des Enfants</td>
                <td>Nombre des Enfants MALADES</td>
                <td>Remarque</td>
                <td>Date de Verification</td>
                <td>Ajouter Remarque</td>
            </tr>
            </thead>
            <tbody>
            {% for f in famille %}
                <tr>
                    <td>{{ f.nomP }}</td>
                    <td>{{ f.verifF }}</td>
                    <td>{{ f.nbrEnf }}</td>
                    <td>{{ f.nbrEnfMalade }}</td>
                    {% if f.remarque == 0 %}
                        <td>pas encore</td>
                    {% else%}
                        <td>{{ f.remarque }}</td>
                    {% endif %}

                    {% if f.verifF=='en cours de verification' or f.VerifF=='non verifiee'%}
                        <td>pas encore</td>
                    {% elseif f.verifF=='verifiee' %}
                        <td>{{ f.dateV.format('d/m/y H') }}</td>
                    {% endif %}
                    <td><a href="{{ path('AjoutR', {'id':f.idF}) }}">
                            <img src="data:image/png;base64,"/>
                        </a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>$
    </div>
{% endblock %}
